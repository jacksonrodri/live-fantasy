                  game1={{\n                                                isPowerUp: true,\n                                             }}\n                                             game2={{\n                                                isPowerUp: true,\n                                             }}\n                                             game3={{\n                                                isPowerUp: true,\n                                             }}\n                                        />\n                                        <PowerPickInfoCard\n                                            game1={{\n                                                isPowerUp: true,\n                                             }}\n                                             game2={{\n                                                isPowerUp: true,\n                                             }}\n                                             game3={{\n                                                isPowerUp: true,\n                                             }}\n                                        />\n                                    </PowerPickCard>\n                                    :\n                                    <>\n                                        <PowerPickInfoCard\n                                            game1={{\n                                                isCell: true,\n                                                cellTitle: \"Bengals +3.5\",\n                                                highCell: true,\n                                            }}\n                                            game2={{\n                                                isCell: true,\n                                                cellTitle: \"Saints to Win\",\n                                                mediumCell: true,\n                                            }}\n                                            game